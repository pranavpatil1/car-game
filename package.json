{
  "name": "lance-gg-project",
  "version": "1.0.0",
  "description": "A multiplayer game project using Lance.gg with Node.js, React, and TypeScript",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --watch src/server --exec ts-node src/server/index.ts",
    "dev:client": "webpack serve --config webpack.config.js --mode development",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "webpack --config webpack.config.js --mode production",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "keywords": [
    "lance",
    "game",
    "multiplayer",
    "typescript",
    "react",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/three": "^0.174.0",
    "express": "^4.18.2",
    "lance-gg": "^4.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket-io": "^1.0.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "three": "^0.174.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^4.0.14",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
